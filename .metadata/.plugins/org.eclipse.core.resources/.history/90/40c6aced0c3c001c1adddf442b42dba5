
public class Perico {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a=5;
		int b=2;
		double primera;
		double segunda;
		
		primera=(5*(Math.pow(a, 2))*(Math.pow(b, 3)));
		segunda=Math.sqrt(Math.pow(a, 2)+(Math.pow(b, 2)));
				
		System.out.println(primera+segunda);
			
		
		
	}

}
